{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",
		"lib": [
			"ES2019"
		],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": "src",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": [
			"mocha",
			"node"
		]
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		".vscode-test",
		"front",
		"out",
		"*.js"
	]
}